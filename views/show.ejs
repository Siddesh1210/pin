<% include ./partials/header.ejs %>  
    <div class="w-full min-h-screen bg-zinc-700 text-white p-5">
        <div class="cards flex flex-wrap gap-10">
            <%user.posts.forEach((post)=>{%>
            <div class="card w-32">
                <div class=" w-full h-40 rounded-lg px-10 py-8 bg-sky-200 overflow-hidden">
                    <img class="h-full w-full object-cover" src="/images/uploads/<%=post.image%>">
                </div>
                <a class="mt-2 inline-block text-sm text-center" href="/post/<%=post._id%>"><%=post.title%></a>
            </div>
            <%})%>
        </div>
    </div>
<% include ./partials/footer.ejs %>  
